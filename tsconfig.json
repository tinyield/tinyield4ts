{
    "extends": "./node_modules/gts/tsconfig-google.json",
    "compilerOptions": {
        "rootDir": "src",
        "sourceMap": true,
        "target": "es5",
        "declaration": true,
        "outDir": "dist",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "alwaysStrict": true,
        "downlevelIteration": true,
        "allowJs": false,
        "strict": true,
        "allowUnreachableCode": false,
        "forceConsistentCasingInFileNames": true,
        "importHelpers": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "noUnusedLocals": true,
        "strictNullChecks": false,
        "lib": [
            "es2016",
            "dom",
            "esnext",
            "esnext.asynciterable"
        ]
    },
    "include": [
        "src/**/*.ts",
        "test/**/*.ts"
    ]
}
